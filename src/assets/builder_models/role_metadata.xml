<?xml version="1.0" encoding="UTF-8"?>
<document xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="https://netgrif.github.io/petriflow/petriflow.schema.xsd">
  <id>role_metadata</id>
  <initials>RME</initials>
  <title>Role Metadata Editor</title>
  <defaultRole>true</defaultRole>
  <transitionRole>false</transitionRole>
  <!-- TRANSACTIONS -->
  <!-- ROLES -->
  <!-- PROCESS ROLE REFS -->
  <!-- PROCESS USER REFS -->
  <!-- DATA -->
  <data type="text">
    <id>role_id</id>
    <title>Id</title>
  </data>
  <data type="text">
    <id>role_name</id>
    <title>Name</title>
  </data>
  <!-- I18NS -->
  <!-- TRANSITIONS -->
  <transition>
    <id>t1</id>
    <x>180</x>
    <y>140</y>
    <layout>
      <offset>0</offset>
    </layout>
    <dataGroup>
      <id>DataGroup_0</id>
      <layout>grid</layout>
      <dataRef>
        <id>role_id</id>
        <logic>
          <behavior>editable</behavior>
          <behavior>required</behavior>
        </logic>
        <layout>
          <x>0</x>
          <y>0</y>
          <rows>1</rows>
          <cols>2</cols>
          <offset>0</offset>
          <template>material</template>
          <appearance>outline</appearance>
        </layout>
      </dataRef>
      <dataRef>
        <id>role_name</id>
        <logic>
          <behavior>editable</behavior>
          <behavior>required</behavior>
        </logic>
        <layout>
          <x>2</x>
          <y>0</y>
          <rows>1</rows>
          <cols>2</cols>
          <offset>0</offset>
          <template>material</template>
          <appearance>outline</appearance>
        </layout>
      </dataRef>
    </dataGroup>
  </transition>
  <!-- PLACES -->
  <!-- ARCS -->
</document>
